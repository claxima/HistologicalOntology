:Corazón
      a       :OrganoCorazon , owl:NamedIndividual ;
      :tieneTunica :TunicaCorazónEndocardio .

:OrganoArteriaMuscularPopitlea
      a       owl:Class ;
      rdfs:comment "prolongación de la arteria femoral"@es ;
      rdfs:label "Arteria Popitlea"@es , "Popliteal Artery"@en ;
      rdfs:subClassOf :OrganoArteriaMuscular .

:OrganoCapilarContinuo
      a       owl:Class ;
      rdfs:comment "no poseen poros ni menestras en sus paredes. Se encuentran en los tejidos muscular, nervioso y conectivo"@es ;
      rdfs:label "Capilar Continuo"@es , "Continuous Capillary"@en ;
      rdfs:subClassOf :OrganoCapilar .

[]    a       owl:AllDisjointClasses ;
      owl:members (:SectorCuerdasTendinosas :SectorMusculo :SectorSistemaConducenteDelCorazón :SectorValvula) .

:TunicaCorazónEndocardio
      a       owl:NamedIndividual , owl:Class , :TunicaCorazónEndocardio ;
