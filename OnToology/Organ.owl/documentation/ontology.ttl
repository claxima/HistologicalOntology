@prefix :        <https://w3id.org/def/Organ#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

:OrganoVenaVénulaColectora
      a       owl:Class ;
      rdfs:comment "30 - 50 micrones, donde podemos observar una capa muscular continua con gran cantidad de glóbulos rojos"@es ;
      rdfs:label "Vénula Colectora"@es , "Collection Venule"@en ;
      rdfs:subClassOf :OrganoVenaVénula .

:OrganoVenaGranCalibreYugular
      a       owl:Class ;
      rdfs:comment " Drena la sangre que proviene en su mayor parte del cuero cabelludo y de la cara."@es ;
      rdfs:label "Vena Yugular"@es , "Jugular Vein"@en ;
      rdfs:subClassOf :OrganoVenaGranCalibre .

:OrganoVenaPequeñoCalibre
      a       owl:Class ;
      rdfs:comment "Venas de menor diametro"@es ;
      rdfs:label "Vena Pequeño Calibre"@es , "Small Caliber Vein"@en ;
      rdfs:subClassOf :OrganoVenaVena .

[]    a       owl:AllDisjointClasses ;
      owl:members (:OrganoArteria :OrganoCapilar :OrganoCorazon :OrganoVena) .

:OrganoVenaGranCalibreSubclavia
      a       owl:Class ;
      rdfs:comment "es la continuación de la vena axilar"@es ;
      rdfs:label "Subclavian Vein"@en , "Vena Subclavia"@es ;
      rdfs:subClassOf :OrganoVenaGranCalibre .

:TunicaCorazónMiocardio
      a       owl:Class ;
      rdfs:comment "Parte muscular del corazón."@es ;
      rdfs:label "Miocardio"@es , "Myocardial"@en ;
      rdfs:seeAlso "http://purl.obolibrary.org/obo/MA_0000080"^^xsd:anyURI ;
      rdfs:subClassOf :TunicaCorazón .

:OrganoVenaMedianoCalibrePopitlea
      a       owl:Class ;
      rdfs:comment " pasa por detrás del rombo poplíteo"@es ;
      rdfs:label "Popliteal Vein"@en , "Vena Popitlea"@es ;
      rdfs:subClassOf :OrganoVenaMedianoCalibre .

:OrganoVenaMedianoCalibreBraquial
      a       owl:Class ;
      rdfs:comment "son venas comitantes de la arteria braquial en el brazo."@es ;
      rdfs:label "Brachial Vein"@en , "Vena Braquial"@es ;
      rdfs:subClassOf :OrganoVenaMedianoCalibre .

:OrganoVenaGranCalibreCavaInferior
      a       owl:Class ;
      rdfs:comment "retorna sangre de los miembros inferiores, los órganos del abdomen y la pelvis hasta la aurícula derecha del corazón"@es ;
      rdfs:label "Inferior Vein Cava"@en , "Vena Cava Inferior"@es ;
      rdfs:subClassOf :OrganoVenaGranCalibre .

:TunicaCorazónPericardioVisceral
      a       owl:Class ;
      rdfs:comment "Cara externa del pericardio"@es ;
      rdfs:label "Pericardio Visceral"@es , "Visceral Pericardium"@en ;
      rdfs:subClassOf :TunicaCorazónPericardio .

:OrganoCapilarDiscontinuo
      a       owl:Class ;
      rdfs:comment "presencia de fenestraciones sin diafragmas"@es ;
      rdfs:label "Discontinuous Capilar"@en , "Capilar Discontinuo"@es ;
      rdfs:subClassOf :OrganoCapilar .

:esSector
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :OrganoCorazon ;
      rdfs:label "is Sector of"@en , "es Sector de"@es ;
      rdfs:range :Sector ;
      owl:inverseOf :tieneSector .

:OrganoVenaVénula
      a       owl:Class ;
      rdfs:comment "pequeñas venas que conducen la sangre desde los capilares hacia las venas."@es ;
      rdfs:label "Vénula"@es , "Venule"@en ;
      rdfs:subClassOf :OrganoVenaPequeñoCalibre ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :Tunica ;
                owl:onProperty :tieneTunica ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :TunicaVasosSanguineosIntima ;
                owl:onProperty :tieneTunica ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] .

:SectorSistemaConducenteDelCorazónFibraDePurkinje
      a       owl:Class ;
      rdfs:comment "forman parte del músculo cardiaco y se localizan en las paredes ventriculares, por debajo del endocardio."@es ;
      rdfs:label "Fibra de Purkinje"@es , "Purkinje Fibers"@en ;
      rdfs:subClassOf :SectorSistemaConducenteDelCorazón .

:Organo
      a       owl:Class ;
      rdfs:comment "es una agrupación de diversos tejidos que forman una unidad estructural encargada del cumplimiento de una función determinada"@es ;
      rdfs:label "Órgano"@es , "Organ"@en ;
      owl:equivalentClass
              [ a       owl:Restriction ;
                owl:onClass :OrganoTipo ;
                owl:onProperty :tieneTipo ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      owl:equivalentClass
              [ a       owl:Restriction ;
                owl:onClass :Sistema ;
                owl:onProperty :esOrganoDe ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] .

:OrganoCapilar
      a       owl:Class ;
      rdfs:comment " vasos sanguíneos de menor diámetro"@es ;
      rdfs:label "Capillary"@en , "Capilar"@es ;
      rdfs:subClassOf :Organo ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :SistemaCirculatorio ;
                owl:onProperty :esOrganoDe ;
                owl:qualifiedCardinality
                        "2"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :OrganoTipo ;
                owl:onProperty :tieneOrganoTipo ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :TunicaVasosSanguineosIntima ;
                owl:onProperty :tieneTunica ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :TunicaVasosSanguineos ;
                owl:onProperty :tieneTunica ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :OrganoTubular ;
                owl:onProperty :tieneTipo ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] .

:SectorValvula
      a       owl:Class ;
      rdfs:comment "Mecanismo que regula el flujo de la comunicación entre dos partes del sistema"@es ;
      rdfs:label "Valve"@en , "Válvula"@es ;
      rdfs:subClassOf :Sector .

:SectorSistemaConducenteDelCorazón
      a       owl:Class ;
      rdfs:comment "permite que el impulso generado en el nodo sinusal (SA) sea propagado y estimule al miocardio (el músculo cardíaco), causando su contracción"@es ;
      rdfs:label "Sistema Conducente del Corazón"@es , "Leading System of Heart"@en ;
      rdfs:subClassOf :Sector .

:OrganoVenaMedianoCalibreRadial
      a       owl:Class ;
      rdfs:comment "su trayectoria a través del dorso de la mano y la cara lateral del antebrazo"@es ;
      rdfs:label "Vena Radial"@es , "Radial Vein"@en ;
      rdfs:subClassOf :OrganoVenaMedianoCalibre .

:tieneTipo
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :Organo ;
      rdfs:label "has Type"@en , "tiene Tipo"@es ;
      rdfs:range :OrganoTipo ;
      owl:inverseOf :esTipoDe .

:OrganoArteriaMuscularUlnar
      a       owl:Class ;
      rdfs:comment "se origina como rama terminal interna de la arteria humeral."@es ;
      rdfs:label "Arteria Ulnar"@es , "Ulnar Artery"@en ;
      rdfs:subClassOf :OrganoArteriaMuscular .

:RegionVentriculoIzquierdo
      a       owl:Class ;
      rdfs:comment "una de las cuatro cavidades del corazón, localizada en la parte inferior e izquireda de éste"@es ;
      rdfs:label "Ventriculo Izquierdo"@es , "Left Ventricle"@en ;
      rdfs:subClassOf :Region .

:OrganoVenaMedianoCalibreTibial
      a       owl:Class ;
      rdfs:comment "se origina como rama de bifurcación anterior de la arteria poplítea"@es ;
      rdfs:label "Tibial Vein"@en , "Vena Tibial"@es ;
      rdfs:subClassOf :OrganoVenaMedianoCalibre .

:OrganoTipo
      a       owl:Class ;
      rdfs:comment "Contiene Tubular y Visceral"@es ;
      rdfs:label "Type of Organ"@en , "Tipo de Órgano"@es .

:SectorValvulaTricuspide
      a       owl:Class ;
      rdfs:comment "Se encuentran en los conductos de salida de las cuatro cavidades del corazón,"@es ;
      rdfs:label "Válvula Tricuspide"@es , "Right Atrioventricular Valve"@en , "Válvula atrioventricular Derecha"@es , "Tricuspid Valve"@en ;
      rdfs:seeAlso "http://purl.obolibrary.org/obo/MA_0000090"^^xsd:anyURI ;
      rdfs:subClassOf :SectorValvula ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :SectorValvula ;
                owl:onProperty :tieneValvula ;
                owl:qualifiedCardinality
                        "3"^^xsd:nonNegativeInteger
              ] .

:OrganoVenaGranCalibreIliaca
      a       owl:Class ;
      rdfs:comment "prolongación de la vena femoral"@es ;
      rdfs:label "Vena Iliaca"@es , "Iliac Vein"@en ;
      rdfs:subClassOf :OrganoVenaGranCalibre .

:tieneSector
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :Sector ;
      rdfs:label "tiene Sector"@es , "has Sector"@en ;
      rdfs:range :OrganoCorazon .

:SectorSistemaConducenteDelCorazónHazDeHis
      a       owl:Class ;
      rdfs:comment " formación intracardíaca consistente en un fino cordón de naturaleza muscular, de aproximadamente 1 cm de longitud, que forma parte del sistema de conducción del corazón, por medio del cual la excitación de las aurículas se trasmite a los ventrículos."@es ;
      rdfs:label "Haz de His"@es , " Bundle of His"@en ;
      rdfs:subClassOf :SectorSistemaConducenteDelCorazón .

:esOrganoTipo
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :OrganoTipo ;
      rdfs:label "is Type Organ"@en , "es Órgano Tipo"@es ;
      rdfs:range :Sistema .

:estaCompuestoDe
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :Tunica ;
      rdfs:label "esta Compuesto de"@es , "is Composed of"@en ;
      rdfs:range :Tunica ;
      owl:inverseOf :esComponenteDe .

:tieneValvula
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :SectorValvula ;
      rdfs:label "has Valve"@en , "tiene Válvula"@es ;
      rdfs:range :Organo ;
      owl:inverseOf :esValvulaDe .

