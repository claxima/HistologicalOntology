      rdfs:label "Endocardio"@es , "Endocardium"@en ;
      rdfs:subClassOf :TunicaCorazón ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :TunicaCorazónSubendotelio ;
                owl:onProperty :estaCompuestoDe ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :TunicaCorazónEndotelio ;
                owl:onProperty :estaCompuestoDe ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass :TunicaCorazónSubendocardio ;
                owl:onProperty :estaCompuestoDe ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] .

:OrganoArteriaElasticaIliaca
      a       owl:Class ;
      rdfs:comment "se originan de la bifurcación de la aorta. Las arterias se extienden hacia abajo"@es ;
      rdfs:label "Iliac Artery"@en , "Arteria Iliaca"@es ;
      rdfs:subClassOf :OrganoArteriaElastica .

:tieneOrgano
