:SectorMusculo
      a       owl:Class ;
      rdfs:comment "Contiene musculos papilared, pectinados y trabéculas cárneas"@es ;
      rdfs:label "Músculo Sector"@es , "Muscle Sector"@en ;
      rdfs:subClassOf :Sector .

:SectorCuerdasTendinosas
      a       owl:Class ;
      rdfs:comment "Fibras tendinosas con forma de salvavidas que atraviesan el corazón y conectan las cúspides de las válvulas AV con el músculo papilar."@es ;
      rdfs:label "Chordae Tendineae"@en , "Cuerdas Tendinosas"@es ;
      rdfs:subClassOf :Sector .

:TunicaCorazónEndotelio
      a       owl:Class ;
      rdfs:comment "Capa endotelial"@es ;
      rdfs:subClassOf :TunicaCorazón .

:OrganoVenaMedianoCalibreUlnar
      a       owl:Class ;
      rdfs:comment "Vena ulnar ."@es ;
      rdfs:label "Ulnar Vein"@en , "Vena Ulnar"@es ;
      rdfs:subClassOf :OrganoVenaMedianoCalibre .

[]    a       owl:AllDisjointClasses ;
      owl:members (:OrganoVenaMedianoCalibreAxilar :OrganoVenaMedianoCalibreBraquial :OrganoVenaMedianoCalibrePalmar :OrganoVenaMedianoCalibrePlantar :OrganoVenaMedianoCalibrePopitlea :OrganoVenaMedianoCalibreRadial :OrganoVenaMedianoCalibreTibial :OrganoVenaMedianoCalibreUlnar) .

:tieneRegion
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :OrganoCorazon ;
      rdfs:label "tiene Región"@es , "has Region"@en ;
      rdfs:range :Region .

:OrganoVenaMedianoCalibrePalmar
      a       owl:Class ;
      rdfs:comment "Recogén sangre de la mano"@es ;
      rdfs:label "Vena Palmar"@es , "Palmar Vein"@en ;
      rdfs:subClassOf :OrganoVenaMedianoCalibre .

:OrganoArteriaMuscularTibial
      a       owl:Class ;
      rdfs:comment "Se origina como rama de bifurcación anterior de la arteria poplítea"@es ;
      rdfs:label "Tibial Artery"@en , "Arteria Tibial"@es ;
      rdfs:subClassOf :OrganoArteriaMuscular .

:esRegionDe
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :Region ;
      rdfs:label "is Region of"@en , "es Región de"@es ;
      rdfs:range :OrganoCorazon ;
      owl:inverseOf :tieneRegion .

:OrganoArteriaMuscularPalmar
      a       owl:Class ;
      rdfs:comment "Situada en la mano"@es ;
      rdfs:label "Palmar Artery"@en , "Arteria Palmar"@es ;
      rdfs:subClassOf :OrganoArteriaMuscular .

:TunicaVasosSanguineosIntima
      a       owl:Class ;
      rdfs:comment "túnica interna"@es ;
      rdfs:label "Inner Layer"@en , "Túnica Intima"@es ;
      rdfs:subClassOf :TunicaVasosSanguineos .

:OrganoVenaVénulaMuscular
      a       owl:Class ;
      rdfs:comment "50 - 100 micrones, aquí ya podemos encontrar claramente fibras musculares lisas formando parte de la pared"@es ;
      rdfs:label "Muscle Venule"@en , "Vénula Muscular"@es ;
      rdfs:subClassOf :OrganoVenaVénula .

:OrganoArteriaMuscular
      a       owl:Class ;
      rdfs:comment "Constituyen las arterias pequeñas y medianas del organismo."@es ;
      rdfs:label "Muscular Artery"@en , "Arteria Muscular"@es ;
      rdfs:subClassOf :OrganoArteria .

:esTunicaDe
      a       owl:ObjectProperty ;
      rdfs:comment "relación de composición o formación"@es ;
      rdfs:domain :Tunica ;
      rdfs:label "es Túnica de"@es , "is Layer of"@en ;
      rdfs:range :Organo .

:OrganoVenaGranCalibreFemoral
      a       owl:Class ;
      rdfs:comment "es la continuación de la vena poplítea"@es ;
      rdfs:label "Femoral Vein"@en , "Vena Femoral"@es ;
      rdfs:subClassOf :OrganoVenaGranCalibre .

:TunicaVasosSanguineos
      a       owl:Class ;
      rdfs:comment "Capas que componen los vasos sanguineos"@es ;
      rdfs:label "Blood Vessels' Layers"@en , "Túnica Vasos Sanguineos"@es ;
      rdfs:subClassOf :Tunica .

